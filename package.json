{
  "name": "react-layout-masonry",
  "version": "1.0.6",
  "description": "Responsive masonry layout for React",
  "type": "module",
  "repository": "https://github.com/sibiraj-s/react-layout-masonry.git",
  "bugs": "https://github.com/sibiraj-s/react-layout-masonry/issues",
  "homepage": "https://github.com/sibiraj-s/react-layout-masonry#readme",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "author": "Sibiraj",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "keywords": [
    "react-masonry",
    "react-layout-masonry"
  ],
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "build": "tsup",
    "prepare": "husky install",
    "prepublishOnly": "is-ci || (npm run build && publint)"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.22",
    "@vitest/coverage-v8": "^0.34.5",
    "commitlint-config-non-conventional": "^1.0.1",
    "eslint": "^8.50.0",
    "eslint-config-pegasus": "^5.0.0",
    "eslint-config-prettier": "^9.0.0",
    "happy-dom": "^12.1.6",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0",
    "prop-types": "^15.8.1",
    "publint": "^0.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.5"
  }
}
